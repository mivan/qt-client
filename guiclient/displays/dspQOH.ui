 <class>dspQOH</class>
 <widget class="QWidget" name="dspQOH">
  <property name="geometry">
   <rect>
    <x>0</x>
    <y>0</y>
    <width>563</width>
    <height>120</height>
   </rect>
  </property>
  <property name="windowTitle">
   <string>Quantities on Hand</string>
  </property>
  <layout class="QGridLayout" name="gridLayout">
   <item row="0" column="0">
    <widget class="QGroupBox" name="_showGroup">
     <property name="title">
      <string/>
     </property>
     <layout class="QVBoxLayout">
      <item>
       <widget class="QRadioButton" name="_showAll">
        <property name="text">
         <string>Show All Quantities</string>
        </property>
        <property name="checked">
         <bool>true</bool>
        </property>
       </widget>
      </item>
      <item>
       <widget class="QRadioButton" name="_showPositive">
        <property name="text">
         <string>Only Show Positive Quantities</string>
        </property>
       </widget>
      </item>
      <item>
       <widget class="QRadioButton" name="_showNegative">
        <property name="text">
         <string>Only Show Negative Quantities</string>
        </property>
       </widget>
      </item>
     </layout>
    </widget>
   </item>
   <item row="0" column="1">
    <widget class="QGroupBox" name="_showValue">
     <property name="enabled">
      <bool>false</bool>
     </property>
     <property name="title">
      <string>Show Inventory Value</string>
     </property>
     <property name="checkable">
