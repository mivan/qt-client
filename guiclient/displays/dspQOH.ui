         <bool>true</bool>
        </property>
       </widget>
      </item>
      <item>
       <widget class="QRadioButton" name="_useStandardCosts">
        <property name="text">
         <string>Use Standard Costs</string>
        </property>
       </widget>
      </item>
      <item>
       <widget class="QRadioButton" name="_useActualCosts">
        <property name="text">
         <string>Use Actual Costs</string>
        </property>
       </widget>
      </item>
     </layout>
    </widget>
   </item>
  </layout>
 </widget>
 <layoutdefault spacing="5" margin="5"/>
 <tabstops>
  <tabstop>_showAll</tabstop>
  <tabstop>_showPositive</tabstop>
